		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.9.4",
		"tslib": "^2.3.1",
		"typescript": "^4.4.3",
		"sveltekit-adapter-aws": "^1.0.22",
		"@yippiecloud/cdk-stack-sveltekit": "^1.0.1",
		"aws-cdk": "^1.129.0"
	},
	"type": "module"
